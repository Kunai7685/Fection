	PlaySound(10066936758, 0.6, 1, false)
	TweenService:Create(SettingsButton, TweenInfo.new(0.45, Enum.EasingStyle.Back, Enum.EasingDirection.Out, 0, false, 0), {
		Rotation = 10
	}):Play()
end)
