								boxEsp.Visible = true
								boxEsp.ZIndex = 5
								coroutine.resume(coroutine.create(function()
									while boxEsp do
										TweenService:Create(boxEsp, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
											Color3 = Color3.fromRGB(255, 255, 210)
										}):Play()
										wait(1)
										TweenService:Create(boxEsp, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
											Color3 = Color3.fromRGB(255, 255, 128)
										}):Play()
										wait(1)
									end
								end))
							end
						end
					end
				end
			end))
			coroutine.resume(coroutine.create(function()
				if v.Name == "FigureRagdoll" then
					for i1,v1 in pairs(v:GetChildren()) do wait()
						if v1:FindFirstChild("BoxHandleAdornment") then
