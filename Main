if _G.SettingsFection then
else
	return
end; if _G.FectionExecuted then
	warn("You've already executed Fection.")
	return
end
_G.FectionExecuted = true
function fromHex(str)return (str:gsub('..', function (cc)return string.char(tonumber(cc, 16))end))end
game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Fection"; Text = "Running.", Duration = 15, Icon = tostring(fromHex("726278617373657469643a2f2f3131373130393833333033"))})
local Player = game.Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Settings = {
	SprintingSpeed = _G.SettingsFection.SprintingSpeed;
	ProximityPromptMaxActivationDistance = _G.SettingsFection.ProximityPromptMaxActivationDistance;
}
SpEnabled = false
Player.Chatted:Connect(function(msg)
	if msg == "!playagain" then
		game.ReplicatedStorage.Bricks.PlayAgain:FireServer()
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Cheat's", Text = ("Playing Again."), Duration = 5})
	elseif msg == "!lobby" then
		game.ReplicatedStorage.Bricks.Lobby:FireServer()
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Cheat's", Text = ("Lobby."), Duration = 5})
	elseif msg == "!revive" then
		game.ReplicatedStorage.Bricks.Revive:FireServer()
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Cheat's", Text = ("Revive."), Duration = 5})
	elseif msg == "!rejoin" then
		local TeleportService = game:GetService("TeleportService")
		TeleportService:Teleport(game.PlaceId, Player)
	elseif msg == "!iy" then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Cheat's", Text = ("Infinite Yield Executed."), Duration = 5})
	end
end)
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
	if not gameProcessedEvent then
		if input.KeyCode == Enum.KeyCode.RightShift then
			SpEnabled = not SpEnabled
			game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Cheat's", Text = ("Sprinting switch to "..Settings.SprintEnabled), Duration = 5})
		end
	end
end)
RunService.RenderStepped:Connect(function()
	if SpEnabled == true then
		Player.Character.Humanoid.WalkSpeed = Settings.SprintingSpeed
		game:GetService("TweenService"):Create(Player.Character.Humanoid, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
			WalkSpeed = Settings.SprintingSpeed
		}):Play()
	end
end)

workspace.Camera.ChildAdded:Connect(function(child)
	if child.Name == "Screech" then
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Warning", Text = ("Screech is watching you!"), Duration = 5})
	end
end)

workspace.ChildAdded:Connect(function(child)
	if child.Name == "RushMoving" then
		if child.RushNew:FindFirstChild("BoxHandleAdornment") then
		else
			local boxEsp = Instance.new("BoxHandleAdornment", child.RushNew)
			boxEsp.Adornee = child.RushNew
			boxEsp.Color3 = Color3.fromRGB(0, 0, 0)
			boxEsp.Transparency = 0.5
			boxEsp.AlwaysOnTop = true
			boxEsp.Size = child.RushNew.Size
			boxEsp.Visible = true
			boxEsp.ZIndex = 5
		end
		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Warning", Text = ("Holy Sh*t, Rush is coming."), Duration = 5})
	elseif child.Name == "SeekMoving" then
		if child.Figure then
			if child.RushNew:FindFirstChild("BoxHandleAdornment") then
			else
				local boxEsp = Instance.new("BoxHandleAdornment", child.Figure)
				boxEsp.Adornee = child.Figure
				boxEsp.Color3 = Color3.fromRGB(255, 0, 4)
				boxEsp.Transparency = 0.5
				boxEsp.AlwaysOnTop = true
				boxEsp.Size = child.Figure.Size
				boxEsp.Visible = true
				boxEsp.ZIndex = 5
			end
		end
	elseif child.Name == "Lookman" then
		if child.Core then
			if child.RushNew:FindFirstChild("BoxHandleAdornment") then
			else
				local boxEsp = Instance.new("BoxHandleAdornment", child.Core)
				boxEsp.Adornee = child.Core
				boxEsp.Color3 = Color3.fromRGB(0, 242, 255)
				boxEsp.Transparency = 0.5
				boxEsp.AlwaysOnTop = true
				boxEsp.Size = child.Core.Size
				boxEsp.Visible = true
				boxEsp.ZIndex = 5
			end
		end
	end
end)

--[[ ---. .--- ]]--[[ -- -- - -- ------- ------- - - - -- .Success Printer. --]] print("Fection Success.".." 1554")
while true do wait(0.5)
	for i,v in pairs(workspace.CurrentRooms:GetDescendants()) do
		if v.Name == "ToUnanchor" then
			if v.Parent.Name == "Main" then
				if v:FindFirstChild("BoxHandleAdornment") then
				else
					local boxEsp = Instance.new("BoxHandleAdornment", v)
					boxEsp.Adornee = v.Parent
					boxEsp.Color3 = Color3.fromRGB(255, 255, 255)
					boxEsp.Transparency = 0.5
					boxEsp.AlwaysOnTop = true
					boxEsp.Size = v.Size
					boxEsp.Visible = true
					boxEsp.ZIndex = 5
				end
				if v.Parent.Parent.Name == "LeverForGate" then
					if v:FindFirstChild("BoxHandleAdornment") then
					else
						local boxEsp = Instance.new("BoxHandleAdornment", v)
						boxEsp.Adornee = v
						boxEsp.Color3 = Color3.fromRGB(255, 255, 128)
						boxEsp.Transparency = 0.5
						boxEsp.AlwaysOnTop = true
						boxEsp.Size = v.Size
						boxEsp.Visible = true
						boxEsp.ZIndex = 5
					end
				end
			end
		elseif v.Name == "PromptHitbox" then
			if v.Parent.Name == "Hitbox" then
				if v.Parent.Parent.Name == "KeyObtain" then
					if v:FindFirstChild("BoxHandleAdornment") then
					else
						local boxEsp = Instance.new("BoxHandleAdornment", v)
						boxEsp.Adornee = v
						boxEsp.Color3 = Color3.fromRGB(255, 255, 255)
						boxEsp.Transparency = 0.5
						boxEsp.AlwaysOnTop = true
						boxEsp.Size = v.Size
						boxEsp.Visible = true
						boxEsp.ZIndex = 5
					end
				end
			end
		elseif v.Name == "Base" then
			if v.Parent.Name == "LiveHintBook" then
				if v:FindFirstChild("BoxHandleAdornment") then
				else
					local boxEsp = Instance.new("BoxHandleAdornment", v)
					boxEsp.Adornee = v
					boxEsp.Color3 = Color3.fromRGB(147, 255, 130)
					boxEsp.Transparency = 0.5
					boxEsp.AlwaysOnTop = true
					boxEsp.Size = v.Size
					boxEsp.Visible = true
					boxEsp.ZIndex = 5
				end
			end
		elseif v.Name == "Lock" then
			if v.Parent.Name == "Door" then
				if v:FindFirstChild("BoxHandleAdornment") then
				else
					local boxEsp = Instance.new("BoxHandleAdornment", v)
					boxEsp.Adornee = v
					boxEsp.Color3 = Color3.fromRGB(255, 255, 255)
					boxEsp.Transparency = 0.5
					boxEsp.AlwaysOnTop = true
					boxEsp.Size = v.Size
					boxEsp.Visible = true
					boxEsp.ZIndex = 5
				end
				if v:FindFirstChild("BoxHandleAdornment") then
				else
					local boxEsp = Instance.new("BoxHandleAdornment", v)
					boxEsp.Adornee = v.Hole
					boxEsp.Color3 = Color3.fromRGB(255, 255, 255)
					boxEsp.Transparency = 0.5
					boxEsp.AlwaysOnTop = true
					boxEsp.Size = v.Size
					boxEsp.Visible = true
					boxEsp.ZIndex = 5
				end
				if v:FindFirstChild("BoxHandleAdornment") then
				else
					local boxEsp = Instance.new("BoxHandleAdornment", v)
					boxEsp.Adornee = v.Metal
					boxEsp.Color3 = Color3.fromRGB(255, 255, 255)
					boxEsp.Transparency = 0.5
					boxEsp.AlwaysOnTop = true
					boxEsp.Size = v.Size
					boxEsp.Visible = true
					boxEsp.ZIndex = 5
				end
				if v:FindFirstChild("BoxHandleAdornment") then
				else
					local boxEsp = Instance.new("BoxHandleAdornment", v)
					boxEsp.Adornee = v.Thing
					boxEsp.Color3 = Color3.fromRGB(255, 255, 255)
					boxEsp.Transparency = 0.5
					boxEsp.AlwaysOnTop = true
					boxEsp.Size = v.Size
					boxEsp.Visible = true
					boxEsp.ZIndex = 5
				end
			end
		elseif v:IsA("ProximityPrompt") then
			v.HoldDuration = 0
			v.MaxActivationDistance = Settings.ProximityPromptMaxActivationDistance
		end
	end
end
