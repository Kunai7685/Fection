	PlaySound(10066931761, 0.65, 1, false)
	if HomeFrameOpened == true then
		if SettingsFrameOpened == false then
			SettingsFrameOpened = true
			TweenService:Create(SettingsFrame, TweenInfo.new(0.45, Enum.EasingStyle.Back, Enum.EasingDirection.Out, 0, false, 0), {
				Position = UDim2.new(0.233, 0,0.498, 0)
			}):Play()
		else
			SettingsFrameOpened = false
			TweenService:Create(SettingsFrame, TweenInfo.new(0.45, Enum.EasingStyle.Back, Enum.EasingDirection.Out, 0, false, 0), {
				Position = UDim2.new(0.233, 0,1.2, 0)
			}):Play()
		end
	end
end)
