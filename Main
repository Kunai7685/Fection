while true do wait(1)
	for i1,v1 in pairs(workspace.CurrentRooms:GetChildren()) do RunService.RenderStepped:Wait()
		for i, v in pairs(v1:GetDescendants()) do
			coroutine.resume(coroutine.create(function()
				if v.Name == "ToUnanchor" then
					if v.Parent.Name == "Main" then
						if v.Parent.Parent.Name == "LeverForGate" then
							if v:FindFirstChild("BoxHandleAdornment") then
							else
								local boxEsp = Instance.new("BoxHandleAdornment", v)
								boxEsp.Adornee = v
								boxEsp.Color3 = Color3.fromRGB(255, 255, 128)
								boxEsp.Transparency = 0.5
								boxEsp.AlwaysOnTop = true
